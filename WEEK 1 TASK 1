void main(){

//boolean to check if it true/false
bool primeNum = true;

//variable declared
int num = 0;


//condition to iterate if input num is prime or not 
  for (int i = 2; i < num; i++) {
  
    if (num % i == 0) {
      primeNum = false;
    }
  }

  if (primeNum) {
    print('prime');
  } else {
    print('not prime');
  }

}
